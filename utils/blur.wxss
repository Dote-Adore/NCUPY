.content-blurred {
    position:relative;
    top:0;
    left:0;
    right:0;
    /* -webkit-filter:blur(20px); */
    filter:blur(9px);
    opacity: 0.2;
    z-index: 0;
}


#blurBtn{
  position: fixed;
  bottom: 0px;
  width: 100%;
  height: 60px;
  z-index: 1;
  overflow:hidden;
  background: #FFFFFF;
}
