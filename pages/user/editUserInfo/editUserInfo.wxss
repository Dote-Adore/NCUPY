@import"/pages/common/cancel&confirmbtn.wxss";
page{
  background-color: #f5f5f5;
}
.div{
  margin-top: 20px;
  padding: 0px 20px 20px 0px; 
  background-color: white;
  box-shadow:0px 5px 10px 5px #eaeaea;
}
.item{
  border-width: 1px 0 0px 0px;
  border-color: #eeeeee;
  border-style: solid;
  padding: 10px;
  overflow: hidden;
  zoom: 1;
  
}
.itemName{
  width: 120px;
  float: left;
}
.areaPicker{
  width: 100px;
  float: left;
}
.text{
  font-size: 17px;
}
.input{
  background-color: #eeeeee;
  padding: 5px;
  border-radius: 3px;
}
.input-name{
  line-height: 32px;
}


